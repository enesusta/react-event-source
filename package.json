{
  "name": "react-event-stream",
  "version": "1.0.2",
  "description": "react-event-stream is custom react hooks library.",
  "author": "enesusta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/enesusta/react-event-stream"
  },
